{
  "name": "endpoint",
  "version": "1.0.0",
  "description": "Mock API endpoints with static JSON response data",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-js",
    "build-js": "mkdir -p src/client/static/dist && webpack --config config/webpack.js",
    "build-js-watch": "npm run build-js -- --watch --colors",
    "start": "node index.js",
    "start-watch": "nodemon index.js",
    "dev": "concurrently --kill-others --prefix name --prefix-colors cyan.bold,yellow.bold --names HTTP,JS 'npm run start-watch' 'npm run build-js-watch'",
    "lint": "eslint src test",
    "test": "node test | tap-spec",
    "test-watch": "nodemon test | tap-spec",
    "cover": "istanbul cover test --print detail",
    "coveralls": "istanbul cover test --print detail && cat coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LINKIWI/endpoint.git"
  },
  "author": "Kevin Lin <developer@kevinlin.info>",
  "license": "MIT",
  "pre-commit": [
    "lint",
    "test"
  ],
  "dependencies": {
    "ajv": "^4.11.2",
    "babel-core": "^6.11.4",
    "body-parser": "^1.15.2",
    "browser-request": "^0.3.3",
    "cookie-parser": "^1.4.3",
    "copy-to-clipboard": "^3.0.5",
    "csjs": "^1.0.6",
    "csjs-inject": "^1.0.1",
    "deep-extend": "^0.4.1",
    "dottie": "^1.1.1",
    "express": "^4.14.0",
    "fingerprintjs2": "^1.4.1",
    "halogen": "^0.2.0",
    "highlight.js": "^9.9.0",
    "http-status-cats": "^1.0.0",
    "is-array": "^1.0.1",
    "is-json": "^2.0.1",
    "morgan": "^1.7.0",
    "nedb": "^1.8.0",
    "pug": "^2.0.0-beta6",
    "range": "0.0.3",
    "raven": "^1.1.1",
    "raven-js": "^3.10.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-favicon": "^0.0.5",
    "react-helmet": "^3.1.0",
    "react-icons": "^2.2.3",
    "react-loading-hoc": "0.0.2",
    "react-router": "^2.6.1",
    "react-syntax-highlighter": "^4.0.1",
    "react-window-state": "^0.2.0",
    "styletron": "^2.5.1",
    "styletron-client": "^2.5.1",
    "styletron-react": "^2.5.2",
    "styletron-utils": "^2.5.1"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-rewire": "^1.0.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "concurrently": "^3.2.0",
    "coveralls": "^2.11.15",
    "enzyme": "^2.6.0",
    "eslint": "^3.3.1",
    "eslint-config-uber-jsx": "^3.0.1",
    "eslint-plugin-react": "^6.1.2",
    "istanbul": "^1.1.0-alpha.1",
    "jsdom": "^9.8.3",
    "nodemon": "^1.11.0",
    "pre-commit": "^1.2.2",
    "pug-cli": "^1.0.0-alpha6",
    "react-addons-test-utils": "^15.4.1",
    "recursive-readdir": "^2.1.0",
    "sinon": "^1.17.6",
    "stylelint": "^7.6.0",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-order": "^0.2.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0",
    "webpack": "^1.13.1"
  }
}
